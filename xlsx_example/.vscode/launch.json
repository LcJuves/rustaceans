    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'xlsx_example'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=xlsx_example",
                    "--package=xlsx_example"
                ],
                "filter": {
                    "name": "xlsx_example",
                    "kind": "bin"
                }
            },
            "args": [
                "tests/exam0.xlsx",
                "-v"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'xlsx_example'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=xlsx_example",
                    "--package=xlsx_example"
                ],
                "filter": {
                    "name": "xlsx_example",
                    "kind": "bin"
                }
            },
            "args": [
                "tests/exam0.xlsx",
                "-v"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}